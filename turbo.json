{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "clean": {},
    "clean:all": {},
    "type:check": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "ci": {
      "cache": false
    }
  }
}
