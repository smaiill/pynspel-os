{
  "name": "pynspel",
  "version": "0.0.0",
  "repository": "https://github.com/smaiill/pynspel.git",
  "author": "smail",
  "license": "CC-BY-NC-SA-4.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "clean:all": "turbo run clean:all && rimraf node_modules",
    "type:check": "turbo run type:check",
    "test": "turbo run test",
    "ci": "eslint . && turbo run ci",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "dependencies": {
    "rimraf": "^5.0.1",
    "turbo": "^1.9.9"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  }
}
